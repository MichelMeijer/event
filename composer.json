{
    "name": "dazzle-php/event",
    "description": "Dazzle Asynchronous Events & Dispatchers.",
    "keywords": [
        "dazzle", "dazzle-php", "event", "event-driven", "event emitter", "emitter", "dispatch"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/kraken-php/event/issues",
        "source": "https://github.com/kraken-php/event"
    },
    "authors": [
        {
            "name":     "Kamil Jamroz",
            "homepage": "https://github.com/khelle"
        },
        {
            "name":     "The contributors",
            "homepage": "http://github.com/dazzle-php/event/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.7",
        "dazzle-php/loop": "0.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8.0 <5.4.0"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\Event\\": "src/Event",
            "Dazzle\\Event\\Test\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
